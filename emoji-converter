def emoji_converter(message):
    words = message.split(" ")
    emojis = {
        ":)": "ğŸ™‚",
        ":(": "ğŸ™",
        ":D": "ğŸ˜ƒ",
        ":c": "ğŸ˜",
        "c:": "â˜ºï¸",
        ":'(": "ğŸ˜­",
        ":O" or ":o": "ğŸ˜®",
        ":l" or ":|" or ":L" : "ğŸ˜",
        "O.o" or "O.o": "ğŸ¥´",

    }
    output = ""
    for word in words:
        output += emojis.get(word, word) + " "
    return output

message = input("")
print(emoji_converter(message))
